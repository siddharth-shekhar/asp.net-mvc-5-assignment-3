
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


        <h1>Data passed using DataView : </h1>
        @{
            string str1 = (string)ViewData["str1"];
            int num1 = (int)ViewData["num1"];
            <h3>@str1</h3>
            <h3>@num1</h3>
            <hr />
        }
        <h1>Data passed using DataBag : </h1>
        @{ 
            string str2 = ViewBag.str2;
            int num2 = ViewBag.num2;
            <h3>@str2</h3>
            <h3>@num2</h3>
        }

@section localContent
{
    <h3>Index.cshtml's local content goes here</h3>
}




